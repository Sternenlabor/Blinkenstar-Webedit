{
    "name": "webedit-react",
    "version": "1.0.2",
    "description": "Webeditor for Blinkenstar",
    "scripts": {
        "test": "npm run build && npm run lint && flow",
        "start": "BABEL_ENV=hot webpack-dev-server --host 0.0.0.0 --hot --inline --progress --history-api-fallback",
        "dev:frontend": "cross-env NODE_ENV=development API_URL=http://localhost:8000 webpack-dashboard -- webpack-dev-server --host 0.0.0.0 --history-api-fallback",
        "dev": "cross-env NODE_ENV=development API_URL=http://localhost:8000 webpack-dev-server --host 0.0.0.0 --history-api-fallback",
        "lint": "eslint src/**/*.js && eslint src/**/*.jsx",
        "build": "cross-env NODE_ENV=production API_URL=/api webpack",
        "backend": "php -S localhost:8000 -t src/api",
        "dev:all": "concurrently \"npm run dev\" \"npm run backend\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sternenlabor/Blinkenstar-Webedit.git"
    },
    "author": "marudor",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Sternenlabor/Blinkenstar-Webedit/issues"
    },
    "homepage": "https://github.com/Sternenlabor/Blinkenstar-Webedit#readme",
    "devDependencies": {
        "@babel/cli": "^7.27.0",
        "@babel/core": "^7.26.10",
        "@babel/plugin-proposal-decorators": "^7.25.9",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-flow": "^7.25.9",
        "@babel/preset-react": "^7.26.3",
        "babel-loader": "^10.0.0",
        "concurrently": "^9.1.2",
        "core-js": "^3.36.1",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "dotenv": "^16.5.0",
        "file-loader": "^6.2.0",
        "flow-bin": "0.268.0",
        "html-webpack-plugin": "^5.5.0",
        "inline-css-loader": "^2.1.2",
        "prettier": "^3.5.3",
        "style-loader": "^4.0.0",
        "svg-inline-loader": "^0.8.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.99.7",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.1"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mohayonao/web-audio-api-shim": "^0.3.0",
        "@mui/icons-material": "^7.0.2",
        "@mui/material": "^7.0.2",
        "@mui/system": "^7.0.2",
        "font-awesome": "^4.7.0",
        "i18next": "^25.0.1",
        "immutable": "^5.1.1",
        "ios-safe-audio-context": "^1.0.1",
        "lodash": "^4.17.21",
        "prop-types": "^15.8.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-i18next": "^15.5.1",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.5.2",
        "redux": "^5.0.1",
        "redux-actions": "3.0.3",
        "redux-promise": "^0.6.0",
        "svg-inline-react": "^3.2.1",
        "uuid-js": "^0.7.5",
        "waud.js": "^1.0.3"
    },
    "resolutions": {
        "prettier": "^3.5.3"
    }
}
