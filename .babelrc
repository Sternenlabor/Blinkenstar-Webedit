{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "browsers": "> 0.25%, not dead" },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    "@babel/preset-flow"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-optional-chaining"
  ]
}